   "source": [
    "function euclidean_distance(x, y)\n",
    "    distance = 0\n",
    "    for j = (1:length(x))\n",
    "        distance = (x[j] - y[j])^2 + distance \n",
    "    end\n",
    "    return distance^(1/2)\n",
    "end"
   ]
