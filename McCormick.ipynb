   "source": [
    "function McCormick(x1, x2)\n",
    "    term1 = sin(x1+x2)\n",
    "    term2 = (x1-x2)^2\n",
    "    term3 = -1.5*x1\n",
    "    term4 = 2.5*x2\n",
    "    sonuc = term1 + term2 + term3 + term4 + 1\n",
    "    return sonuc\n",
    "end"
   ]
  }
