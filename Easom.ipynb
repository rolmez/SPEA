   "source": [
    "function EasomFunction(x1, x2)\n",
    "    fact1 = -cos(x1) * cos(x2)\n",
    "    fact2 = exp(-(x1-pi)^2-(x2-pi)^2)\n",
    "    y = fact1*fact2\n",
    "    return y\n",
    "end"
   ]
  }
